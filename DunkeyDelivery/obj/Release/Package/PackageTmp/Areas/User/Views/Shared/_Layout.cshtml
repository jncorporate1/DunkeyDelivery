            //document.getElementById("defaultOpen").click();
            function login_first() {
                //  $("#login_first").show().delay(2000).fadeOut();
                $('#login_first')
                  .delay(1000)
                  .queue(function (next) {
                      $(this).css('visibility', 'visible');
                      next();
                  });
            }
            function invisibility_Animation(element_Id, Message) {
                $('#' + element_Id).text(Message);
                $('#' + element_Id)
                 .delay(1000)
                 .queue(function (next) {
                     $(this).css('visibility', 'visible');
                     next();
                 });
            }

