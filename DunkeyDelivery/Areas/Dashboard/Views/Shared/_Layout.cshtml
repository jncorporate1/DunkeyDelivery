<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Dunkey Delivery | Admin Panel </title>
    <!-- Bootstrap -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet">
    <link rel="icon" href="~/Content/assets/images/favicon.ico" type="image/x-icon">
    <!-- Font Awesome -->
    <link href="~/Content/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/animate/animate.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="~/Content/nprogress/nprogress.css" rel="stylesheet">
    <!-- iCheck -->
    <link href="~/Content/iCheck/skins/flat/green.css" rel="stylesheet">

    <!-- bootstrap-progressbar -->
    <link href="~/Content/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- JQVMap -->
    <link href="~/Content/jqvmap/dist/jqvmap.min.css" rel="stylesheet" />
    <!-- bootstrap-daterangepicker -->
    <link href="~/Content/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    <!-- Custom Theme Style -->
    <link href="~/Content/admin-custom/css/custom.css" rel="stylesheet">

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <script src="~/Scripts/admin-custom/js/AdminCustom.js"></script>
    <link href="~/Content/bootstrap-wysiwyg/css/style.css" rel="stylesheet">

</head>


<body class="nav-md">
    <div class="container body" style="max-width:100%;">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">

                        <a href="@Url.Action("Index","Index",new { area="Dashboard"})" class="site_title" style="font-size: -3px;display: inline-flex;font-size: 15px;padding-left: 15px;"><img src="~/Content/assets/images/fav.png" style="height:40px;width:40px;margin-top:10px;margin-right: 15px;" class="img-responsive"/> DUNKEY <span style="color:#6ebe44;">&nbsp; DELIVERY </span></a>
                   
                        <hr style="margin-top:0px;margin-bottom:0px;" />

                    </div>
                    <div class="clearfix"></div>
                    <!-- menu profile quick info -->
                    <div class="profile clearfix">
                       
                        <div class="profile_pic">
                            <img src='@Url.Content(@Model.ProfilePictureUrl)' alt="Profile Image" class="img-circle profile_img">
                        </div>
                        <div class="profile_info">
                            <span>Welcome,</span>
                            <h2>@Model.UserName</h2>
                        </div>
                    </div>
                    <!-- /menu profile quick info -->
                    <br />
                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            <h3>General</h3>
                            <ul class="nav side-menu">
                                <li>
                                    <a href="@Url.Action("Index","Index",new { area="Dashboard"})"><i class="fa fa-home"></i> Dashboard</a>
                    
                                </li>
                              
                                <li>
                                    <a><i class="fa fa-opencart"></i> Stores <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li>
                                            <a href="@Url.Action("Index","Store")">Add New Store</a>
                                        </li>
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-sitemap"></i> Categories <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="@Url.Action("AddCategory","Categories")">Add New Category</a></li>
                                        @*<li><a href="@Url.Action("Manage","Categories")">Manage Categories</a></li>*@
                                    </ul>
                                </li>
                                <li>
                                    <a><i class="fa fa-shopping-bag"></i> Products <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="@Url.Action("Index","Product")">Add New Product</a></li>
                                    </ul>
                                </li>

                                <li>
                                    <a><i class="fa fa-users"></i> Users <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="@Url.Action("Index","User")">Add New User</a></li>
                              
                                    </ul>
                                </li>

                            </ul>
                        </div>
                    </div>
                    <!-- /sidebar menu -->

                </div>
            </div>
            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    @Model.UserName
                                    <span class=" fa fa-angle-down"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-usermenu pull-right">
                                 
                                    <li><a href="@Url.Action("LogOff", null, new { area = string.Empty, controller = "Account" }, Request.Url.Scheme)"><i class="fa fa-sign-out pull-right"></i> Log Out</a></li>

                                </ul>
                            </li>
                            <li role="presentation" class="dropdown">
                                <a href="javascript:;" class="dropdown-toggle info-number" data-toggle="dropdown" aria-expanded="false">
                                    <i class="fa fa-envelope-o"></i>
                                    <span class="badge bg-green">2</span>
                                </a>
                                <ul id="menu1" class="dropdown-menu list-unstyled msg_list" role="menu">
                                    <li>
                                        <a>
                                            <span class="image"><img src="images/img.jpg" alt="Profile Image" /></span>
                                            <span>
                                                <span>Application Admin</span>
                                                <span class="time">3 mins ago</span>
                                            </span>
                                            <span class="message">
                                                New Store Request Submitted
                                            </span>
                                        </a>
                                    </li>
                                    <li>
                                        <a>
                                            <span class="image"><img src="images/img.jpg" alt="Profile Image" /></span>
                                            <span>
                                                <span>Application Admin</span>
                                                <span class="time">1 mins ago</span>
                                            </span>
                                            <span class="message">
                                                New Product Added Into Sepenzo Pizza
                                            </span>
                                        </a>
                                    </li>
                                   
                                   
                                    <li>
                                        <div class="text-center">
                                            <a>
                                                <strong>See All Alerts</strong>
                                                <i class="fa fa-angle-right"></i>
                                            </a>
                                        </div>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->




            @RenderBody();

            <!-- Content Goes Here-->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                    @*Gentelella - Bootstrap Admin Template by <a href="https://colorlib.com">Colorlib</a>*@
                    <span style="font-size:15px">Powered by    </span> <a href="https://www.ingic.ae/" target="_blank"><img height="20px" width="70px" src="~/Content/images/ingic.png"></a>
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>

    @*@Scripts.Render("~/bundles/modernizr")
        @Scripts.Render("~/bundles/fastclick")
        @Scripts.Render("~/bundles/nprogress")
        @Scripts.Render("~/bundles/Chart")
        @Scripts.Render("~/bundles/gauge")
        @Scripts.Render("~/bundles/bootstrap-progressbar")
        @Scripts.Render("~/bundles/icheck")
        @Scripts.Render("~/bundles/skycons")
        @Scripts.Render("~/bundles/flot")
        @Scripts.Render("~/bundles/date")
        @Scripts.Render("~/bundles/daterangepicker")
        @Scripts.Render("~/bundles/custom")*@


    <!-- jQuery -->
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="~/Scripts/fastclick/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/Scripts/nprogress/nprogress.js"></script>
    <!-- Chart.js -->
    <script src="~/Scripts/Chart/Chart.min.js"></script>
    <!-- gauge.js -->
    <script src="~/Scripts/guage/gauge.js"></script>
    <!-- bootstrap-progressbar -->
    <script src="~/Scripts/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <!-- iCheck -->
    <script src="~/Scripts/icheck/icheck.min.js"></script>
    <!-- Skycons -->
    <script src="~/Scripts/skycons/skycons.js"></script>
    <!-- Flot -->
    <script src="~/Scripts/flot/jquery.flot.js"></script>
    <script src="~/Scripts/flot/jquery.flot.pie.js"></script>
    <script src="~/Scripts/flot/jquery.flot.time.js"></script>
    <script src="~/Scripts/flot/jquery.flot.stack.js"></script>
    <script src="~/Scripts/flot/jquery.flot.resize.js"></script>
    <script src="~/Content/bootstrap-wysiwyg/js/bootstrap-wysiwyg.min.js"></script>
    <!-- DateJS -->
    <script src="~/Scripts/date/date.js"></script>

    <script src="~/Scripts/custom/custom.min.js"></script>
    <script src="~/Scripts/validators.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>





</body>
</html>
